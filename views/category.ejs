<!DOCTYPE html>
<html>
<head>
   <title><%= category.name %></title>
   <% include partials/head %>
</head>
<body>
   <h1><%= category.name %></h1>
   <ul id="links">
      <% posts.forEach(function(post) { %>
         <li><a href="<%= post.url %>"><%= post.title %></a>
      <% }); %>
   </ul>
</body>
</html>
